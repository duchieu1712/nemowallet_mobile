        "lint": "eslint . --fix",
        "lint-fix": "eslint . --fix",
        "format": "prettier --write .",
        "type-check": "tsc --pretty --noEmit",
        "start": "react-native start --reset-cache",
        "test": "jest",
        "start_port": "react-native start --port 8084 --reset-cache",
        "android_port": "react-native run-android --port 8084",
        "deep_link": "npx uri-scheme open 'https://testnet.nemoverse.io/marketplace/box/1562/?serviceID=10000' --android",
        "link_react_native_google_signin": "react-native link react-native-google-signin",
        "assets": "react-native bundle --platform android --dev false --entry-file index.tsx --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
