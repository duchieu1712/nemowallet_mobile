        "android:staging:debug": "react-native run-android --mode=stagingDebug",
        "android:dev:debug": "react-native run-android --mode=developmentDebug --appIdSuffix=development",
        "android:prod:debug": "react-native run-android --mode=productionDebug",
        "android:staging:release": "react-native run-android --mode=stagingRelease",
        "android:dev:release": "react-native run-android --mode=developmentRelease --appIdSuffix=development",
        "android:prod:release": "react-native run-android --mode=productionRelease",
        "android:staging:release_aab": "cd android && ./gradlew bundlestagingRelease",
        "android:development:release_aab": "cd android && ./gradlew bundledevelopmentRelease",
        "android:production:release_aab": "cd android && ./gradlew bundleproductionRelease",
        "android:staging:release_apk": "cd android && ./gradlew assemblestagingRelease",
        "android:development:release_apk": "cd android && ./gradlew assembledevelopmentRelease",
        "android:production:release_apk": "cd android && ./gradlew assembleproductionRelease"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "yarn run type-check"
        }
    },
